package awerhg;

import java.util.Scanner;
import java.text.DecimalFormat;

public class dbfg {
	
	final static double k = 9*Math.pow(10, 9);
	
	static double sila (double q1, double q2, double r) {
		return k*((q1*q2)/Math.pow(r, 2));
	}
	
	public static void main(String[] args) {
		
		double q1 = 3 * Math.pow(10, 6);
		double q2 = 3.7 * Math.pow(10, 6);
		double r = Math.pow(10, 4);
		
		System.out.println(sila(q1, q2, r));
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}}
