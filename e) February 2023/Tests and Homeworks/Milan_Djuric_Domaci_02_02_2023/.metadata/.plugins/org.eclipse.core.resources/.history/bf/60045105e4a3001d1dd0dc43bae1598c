package djuric;

import java.util.*;

public class Mejn {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		List<Integer> x = new ArrayList<Integer>();
		Iterator<Integer> z = x.iterator();
		
		int niz[] = {1, 2, 3, 2, 1, 4, 5, 6, 5, 7};
		
		for(int i : niz) {
			x.add(i);
		}
		

		
		System.out.print(x);


	}

}
