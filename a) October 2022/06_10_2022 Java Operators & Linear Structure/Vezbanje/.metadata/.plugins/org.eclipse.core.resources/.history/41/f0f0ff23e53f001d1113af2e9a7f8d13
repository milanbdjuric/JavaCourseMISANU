import java.util.Scanner;

public class zdtk {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner input = new Scanner(System.in);
		
		System.out.println("Unesi koordinatu tacke x1");
		Double x1 = input.nextDouble();
		System.out.println("Unesi koordinatu tacke y1");
		Double y1 = input.nextDouble();
		System.out.println("Unesi koordinatu tacke x2");
		Double x2 = input.nextDouble();
		System.out.println("Unesi koordinatu tacke y2");
		Double y2 = input.nextDouble();
		System.out.println("Vrednost a1 je: " + Math.sqrt(Math.pow(x1, 2) + Math.pow(y1, 2)));
		System.out.println("Vrednost a2 je: " + Math.sqrt(Math.pow(x2, 2) + Math.pow(y2, 2)));


	}

}
