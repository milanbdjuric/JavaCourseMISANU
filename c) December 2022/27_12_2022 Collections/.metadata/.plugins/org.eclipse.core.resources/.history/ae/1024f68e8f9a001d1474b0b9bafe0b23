package dj;

import java.util.*;

public class Mjn {

	public static void main(String[] args) {
		
		List<String> kurton = new LinkedList<String>();
		
		String[] kurac = {"kurac", "palac", "kriminalac", "adasd", "sdfsdfsd", "sdfjiosdfjio", "svkoasdwe", "wefmklwefk", "wefmklefk"};
		
		for(String s : kurac) {
			kurton.add(s);	
		}
		
		ListIterator<String> it = kurton.listIterator();
		
		while(it.hasNext()) {
			System.out.println(it.next());
		}
		
		kurton.remove(2);
		
		System.out.println(kurton);
		
		String x = kurton.get(0);
		System.out.println(x);
		
		kurton.subList(1, 8).clear();
		
		while(it.hasNext()) {
			System.out.println(it.next() + " ");
		}

	}

}
