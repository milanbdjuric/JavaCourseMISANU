package djuric;

public class Mejn {

	public static void main(String[] args) {
		
		Thread t = new Thread();
		
		System.out.println("THREAD: " + t.currentThread());
		System.out.println("ID: " + t.getId());
		System.out.println("IME: " + t.getName());
		System.out.println("PRIORITET: " + t.getPriority());

	}

}
