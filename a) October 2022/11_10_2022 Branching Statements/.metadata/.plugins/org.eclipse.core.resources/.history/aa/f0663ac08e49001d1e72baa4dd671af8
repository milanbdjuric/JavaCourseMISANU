package milan.djuric;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner xyz = new Scanner(System.in);
		
		System.out.println("Unesite znak: ");
		
		char a = xyz.next().charAt(0);
		
		if (a >= 'A' && a <= 'Z')
		{
			System.out.println("Znak je veliko slovo.");
		}
		else if (a >= 'a' && a <= 'z')
		{
			System.out.println("Znak je malo slovo.");
		}
		else if (a >= '0' && a <='9')
		{	
			System.out.println("Znak je cifra.");
		}
		else
		{
			System.out.println("Znak je specijalni znak.");
		}

	}

}
