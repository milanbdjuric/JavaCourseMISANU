package djuric;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Unesite kvadrant: ");
		
		int kvadrant = sc.nextInt();
		
		switch(kvadrant)
		{
		case 1: {
			System.out.println("Moguce koordinate su: 0-90");
			break;
		}
		case 2: {
			System.out.println("Moguce koordinate su: 90-180");
			break;
		}
		case 3: {
			System.out.println("Moguce koordinate su: 180-270");
			break;
		}
		case 4: {
			System.out.println("Moguce koordinate su: 270-360");
			break;
		}
		default:
			System.out.println("Ne moze bre preko 360.");
	}
	}

}
