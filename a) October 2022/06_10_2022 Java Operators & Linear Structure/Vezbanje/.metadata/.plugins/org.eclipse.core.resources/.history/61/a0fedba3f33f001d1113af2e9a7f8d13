import java.util.Scanner;

public class zdtk {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner input = new Scanner(System.in);
		
		System.out.println("Ucitaj dvocifren broj: ");
		
		int a = input.nextInt();
		
		System.out.println("Razlika desetica i jedinica je " + (a / 10 - a % 10));
		
		
		
	}

}
