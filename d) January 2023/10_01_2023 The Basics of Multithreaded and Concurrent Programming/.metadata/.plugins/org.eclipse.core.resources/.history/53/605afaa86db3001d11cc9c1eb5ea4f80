package fwefwe;

public class Mijijijijen {

	public static void main(String[] args) {
		
		A kurac = new A("KURAC");
		A picka = new A("PICKA");
		A govno = new A("GOVNO");
		
		B be = new B();
		
		Thread sisa = new Thread(be, "SISA");

		
		
		govno.start();
		picka.start();
		kurac.start();
		sisa.start();


	}

}
