package djuric;

import java.util.Scanner;

public class Mejn {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Unesite broj: ");
		int x = sc.nextInt();
		
		int y, s = 0;
		
		while(x!=0) {
			y = x%10;
			s+=y;
			x/=10;
		}
		
		System.out.println("S = " + s);
		
		
		
		
		

	}

}
