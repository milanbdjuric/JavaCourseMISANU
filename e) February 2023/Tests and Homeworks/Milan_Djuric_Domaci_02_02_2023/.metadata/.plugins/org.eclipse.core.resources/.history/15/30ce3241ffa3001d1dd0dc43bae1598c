package djuric;

public class Djak extends Osoba {
	
	String nazivSkole, razred;

	Djak(String i, String d, String a, String n, String r) {
		super(i, d, a);
		nazivSkole = n;
		razred = r;
	}

	public String toString() {
		return super.toString() + "\nNaziv skole: " + nazivSkole + "\nRazred: " + razred;
	}
	
	

}
