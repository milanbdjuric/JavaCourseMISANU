
public class Inkrement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i = 3, j = 4;
		
		System.out.println("i = " + i + " j = " + j);
		
		j = 3 + i--; // j = 3 + (i - 1) umanji i pa izračunaj
		
		System.out.println("i = " + i + " j = " + j);
		
		// menjaj ++ i -- mesta, pre i posle pa vidi kako ide

	}

}
