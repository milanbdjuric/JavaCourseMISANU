package djuric;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Unesite tri broja: ");
		
		int br1 = sc.nextInt();
		int br2 = sc.nextInt();
		int br3 = sc.nextInt();
		
		int max = br1;
		
		if (br2 > max) {
			br2 = max;
		}
		if (br3 > max) {
			br3 = max;
		}
		
		System.out.println("Najveci broj je: " + max);

	}

}
